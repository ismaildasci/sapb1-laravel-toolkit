# SAP B1 Integration Test Configuration
# Copy this file to .env.testing and fill in your values

# Enable integration tests
SAPB1_INTEGRATION_ENABLED=true

# SAP B1 Service Layer Connection (from laravel-sapb1 SDK)
# NOTE: Do NOT include /b1s/v1 in the URL - SDK adds it automatically
SAP_B1_URL=https://your-server:50000
SAP_B1_COMPANY_DB=SBODEMOUS
SAP_B1_USERNAME=manager
SAP_B1_PASSWORD=your-password
SAP_B1_VERIFY_SSL=false
SAP_B1_LANGUAGE=23

# Test Data - Existing codes in your SAP B1 database
SAPB1_TEST_CUSTOMER=C20000
SAPB1_TEST_SUPPLIER=V10000
SAPB1_TEST_ITEM=A00001
SAPB1_TEST_WAREHOUSE=01
